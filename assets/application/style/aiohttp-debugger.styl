html, body
    position absolute
    width 100%
    height 100%

body
    display flex

.box
    display flex
    flex-direction column
    flex 1
    max-width 100%
    
.box-body-content
    padding 15px 0

.box-body
    flex 1

.pointer
    cursor pointer

.z-index-1
    z-index 1

.text-small
    font-size 1rem    
    
.shadow-sm
    z-index: 1
    
.icon-th
    text-align center
    width 50px

.icon-td
    text-align center
